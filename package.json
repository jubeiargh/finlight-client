{
  "name": "finlight-client",
  "version": "3.0.2",
  "description": "Finlight.me client for API and WebSocket",
  "homepage": "https://finlight.me",
  "author": "Ali Büyükkakac",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jubeiargh/finlight-client.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "node scripts/update-version.js",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:unit": "jest",
    "test:integration": "jest --runInBand --testPathIgnorePatterns='/node_modules/' --testMatch='**/*.integration.test.ts'",
    "test:integration:api": "jest --runInBand --testPathIgnorePatterns='/node_modules/' --testMatch='**/api.integration.test.ts'",
    "test:integration:ws": "jest --runInBand --testPathIgnorePatterns='/node_modules/' --testMatch='**/websocket.integration.test.ts'",
    "test:all": "jest --runInBand --testPathIgnorePatterns='/node_modules/'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.14.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "^30.0.5",
    "prettier": "3.3.2",
    "ts-jest": "^29.4.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tslib": "2.6.3",
    "typescript": "5.5.2"
  }
}
